create table find_even (id int)

insert into find_even (id) values (1),(3),(5),(6),(7),(9),(10);


with recursive cte as ( 
	select min(id) as id from find_even
					   union
	select id + 1 from cte 
	where id < (select max(id) from find_even)
	)
select id from cte 
where id not in (select id from find_even)
